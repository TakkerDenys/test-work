nk class=\"text-reset\" :to=\"{ name: 'user', params: { id: user._id } }\">\n                                {{ user.name + \" \" + user.surname }}</router-link>\n                        </td>\n                        <td>{{ user.email }}</td>\n                        <td>{{ user.phone }}</td>\n                        <td><strong class=\"text-danger\">NULL</strong></td>\n                        <td><strong class=\"text-danger\">NULL</strong></td>\n                    </tr>\n                </tbody>\n            </table>\n            <div class=\"btn-toolbar\">\n                <div class=btn-group>\n                    <button class=\"btn-primary\" v-for=\"page in pagination.pages\" @click.prevent=\"setPage(page)\">\n                        {{ page }}\n                    </button>\n                </div>\n            </div>\n            <p>Display result from {{ pagination.startIndex }} to {{ pagination.endIndex }}</p>\n        </div>\n        <!-- <b-container>\n            <b-pagination v-model=\"currentPage\" :total-rows=\"rows\" :per-page=\"perPage\">\n            </b-pagination>\n        </b-container>\n        <div class=\"text-center mt-4\">\n            <router-link class=\"nav-link\" to=\"/add-user\"><button type=\"button\"\n                    class=\"btn btn-success btn-lg pl-5 pr-5 pt-3 pb-3 ml-3 mr-3\"><span class=\"h3\">Add new\n                        user</span></button></router-link>\n        </div> -->\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nconst url = \"/api/user/\";\n\nexport default {\n    name: 'Home',\n    data() {\n        return {\n            users: [],\n            perPage: 3,\n            pagination: {}\n\n        };\n    },\n    created() {\n        axios\n            .get(url)\n            .then(response => (this.users = response))\n            .catch(error => console.log(error));\n    },\n    computed:{\n        collection(){\n            return this.paginate(this.data);\n        }\n    },\n    methods:{\n        setPage(page){\n            this.pagination = this.paginator(this.data.lenght, page);\n        },\n        paginate(data){\n            return _.slice(data, this.pagination.sta)\n        }\n    }\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}